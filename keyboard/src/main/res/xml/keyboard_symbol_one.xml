<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="1%p"
    android:keyWidth="10%p"
    android:keyHeight="@dimen/keyboard_key_height"
    android:verticalGap="@dimen/keyboard_verticalGap">

    <Row>
        <Key
            android:codes="49"
            android:keyWidth="9%p"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:keyWidth="9%p"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:keyWidth="9%p"
            android:keyLabel="3" />
        <Key
            android:codes="52"
            android:keyWidth="9%p"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:keyWidth="9%p"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:keyWidth="9%p"
            android:keyLabel="6" />
        <Key
            android:codes="55"
            android:keyWidth="9%p"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:keyWidth="9%p"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:keyWidth="9%p"
            android:keyLabel="9" />
        <Key
            android:codes="48"
            android:keyWidth="9%p"
            android:keyEdgeFlags="right"
            android:keyLabel="0" />
    </Row>

    <Row>
        <Key
            android:codes="45"
            android:keyWidth="9%p"
            android:keyEdgeFlags="left"
            android:keyLabel="-" />
        <Key
            android:codes="47"
            android:keyWidth="9%p"
            android:keyLabel="/" />
        <Key
            android:codes="58"
            android:keyWidth="9%p"
            android:keyLabel=":" />
        <Key
            android:codes="59"
            android:keyWidth="9%p"
            android:keyLabel=";" />
        <Key
            android:codes="40"
            android:keyWidth="9%p"
            android:keyLabel="(" />
        <Key
            android:codes="41"
            android:keyWidth="9%p"
            android:keyLabel=")" />
        <Key
            android:codes="36"
            android:keyWidth="9%p"
            android:keyLabel="$" />
        <Key
            android:codes="38"
            android:keyWidth="9%p"
            android:keyLabel="@string/keyboard_key2" /><!--&-->
        <Key
            android:codes="64"
            android:keyWidth="9%p"
            android:keyLabel="@string/keyboard_key1" /><!--@-->
        <Key
            android:codes="34"
            android:keyWidth="9%p"
            android:keyEdgeFlags="right"
            android:keyLabel="@string/keyboard_key3" /> <!--"-->
    </Row>

    <Row>
        <Key
            android:codes="-37"
            android:keyWidth="14%p"
            android:keyEdgeFlags="left"
            android:keyLabel="#+=" />
        <Key
            android:codes="46"
            android:keyWidth="9%p"
            android:keyLabel="." />
        <Key
            android:codes="44"
            android:keyWidth="9%p"
            android:keyLabel="," />
        <Key
            android:codes="63"
            android:keyWidth="9%p"
            android:keyLabel="\?" />
        <Key
            android:codes="33"
            android:keyWidth="9%p"
            android:keyLabel="!" />
        <Key
            android:codes="39"
            android:keyWidth="9%p"
            android:keyLabel="'" />
        <Key
            android:codes="-5"
            android:keyWidth="14%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/icon_keyboard_del_default" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="-38"
            android:keyWidth="19%p"
            android:keyEdgeFlags="left"
            android:keyLabel="ABC" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyWidth="58%p"
            android:keyLabel="空格" />
        <Key
            android:codes="10"
            android:keyWidth="19%p"
            android:keyEdgeFlags="right"
            android:keyLabel="换行" />
    </Row>
</Keyboard>